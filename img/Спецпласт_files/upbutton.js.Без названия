'use strict';





(function(){

var a=window.constructorMode&&!window.previewMode,

b=function(){return function(d){












function e(m,n){
n==l||(



g.fadeTo(m,n),
l=n);
}var f=d('.ul-upbutton'),g=d('.ul-upbutton-icon',f),h=d('#ul-main'),i=a?0.4:0,j=f.is(':visible'),k=!1,l=0;
















































return f.click(function(){(h[0]?h:d(document)).scrollTop()>=window.innerHeight/2&&(h[0]?h:d('body, html')).animate({scrollTop:0},200)}),f.mouseenter(function(){!j||(h[0]?h:d(document)).scrollTop()>=window.innerHeight/2&&e(100,1)}),f.mouseleave(function(){j&&((h[0]?h:d(document)).scrollTop()<window.innerHeight/2?e(100,i):e(100,0.6),k=!1)}),(h[0]?h:d(window)).on('scroll.upbutton',function(){j&&((h[0]?h:d(document)).scrollTop()<window.innerHeight/2?e(100,i):e(100,Math.max(0.6,g.css('opacity'))))}),e(0,i),{
show:function(){return function(){
j||(


f.show(),
g.fadeTo(0,i),
j=!0);
}}(),
hide:function(){return function(){
j&&(


f.hide(),
j=!1);
}}()};

}}();


a?
define('upbutton',['jquery'],b):

require(['jquery'],function(c){b(c)});


})();
